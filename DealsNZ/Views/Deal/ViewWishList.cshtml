@model IEnumerable<DealsNZ.Models.DealsModels.DealViewModel>

@{
    ViewBag.Title = "Wish List";
    
}




<div class="row">


    <div class="card grey lighten-1">
        <div class="card-content black-text accent-4">
            <span class="card-title center">Wishlist</span>
            <div class="row">
                @foreach (var item in Model)
                {

                    <div>
                        @Html.Partial("~/Views/Shared/DealViews/DealBox.cshtml", item)
                    </div>
                }
            </div>
        </div>
    </div>
</div>

